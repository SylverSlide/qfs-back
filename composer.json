{
  "name": "ankurk91/laravel-blogger-demo",
  "type": "project",
  "description": "The Laravel Blogger.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.4|^8.0",
    "fideloper/proxy": "^4.4",
    "fruitcake/laravel-cors": "^2.0",
    "intervention/image": "^2.5",
    "laravel/framework": "^8.12",
    "laravel/sanctum": "^2.9",
    "laravel/tinker": "^2.5"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.5",
    "barryvdh/laravel-ide-helper": "^2.9",
    "facade/ignition": "^2.5",
    "fakerphp/faker": "^1.9.1",
    "friendsofphp/php-cs-fixer": "^2.18",
    "laravel/sail": "^1.0.1",
    "mockery/mockery": "^1.4.2",
    "nunomaduro/collision": "^5.0",
    "permafrost-dev/phpcsfixer-preset": "^1.5",
    "phpunit/phpunit": "^9.3.3"
  },
  "replace": {
    "paragonie/random_compat": "*",
    "ralouphie/getallheaders": "*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-intl-idn": "*",
    "symfony/polyfill-intl-grapheme": "*",
    "symfony/polyfill-intl-normalizer": "*",
    "symfony/polyfill-mbstring": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "post-update-cmd": [
      "composer ide-helper"
    ],
    "ide-helper": [
      "@php artisan ide-helper:eloquent --ansi",
      "@php artisan ide-helper:generate --ansi",
      "@php artisan ide-helper:meta --ansi",
      "@php artisan ide-helper:models -N --ansi"
    ],
    "clear-all": [
      "@php artisan cache:clear --ansi",
      "@php artisan route:clear --ansi",
      "@php artisan view:clear --ansi",
      "@php artisan config:clear --ansi",
      "@php artisan clear-compiled --ansi",
      "composer dump-autoload -o"
    ],
    "cache-all": [
      "@php artisan config:cache --ansi",
      "@php artisan route:cache --ansi",
      "@php artisan view:cache --ansi"
    ],
    "reset": [
      "composer clear-all",
      "composer cache-all"
    ],
    "test": "./vendor/bin/phpunit --testdox --color=always",
    "worker": "@php artisan queue:work --sleep=3 --ansi",
    "format": "./vendor/bin/php-cs-fixer fix --ansi"
  }
}
